  object zqrBarangKeluar: TZReadOnlyQuery
    Connection = DM.zConn
    SQL.Strings = (
      
        'SELECT a.id, a.no_bukti,  a.tanggal, a.user, a.user_dept, a.kete' +
        'rangan, a.f_ppn, a.no_sj, a.no_faktur, a.jenistrs, a.f_posting'
      'FROM tbl_trskeluar_head a')
    Params = <>
    Left = 647
