  object zqrBarangDet: TZReadOnlyQuery
    Connection = DM.zConn
    SQL.Strings = (
      'SELECT * '
      'FROM v_hist_brg_det'
      'WHERE id_brg = :id_brg AND id_gdg = :id_gdg'
      'ORDER BY id DESC')
    Params = <
      item
        DataType = ftUnknown
        Name = 'id_brg'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'id_gdg'
        ParamType = ptUnknown
      end>
    Left = 728
    Top = 344
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'id_brg'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'id_gdg'
        ParamType = ptUnknown
      end>
  end
