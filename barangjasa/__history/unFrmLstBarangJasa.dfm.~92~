  end
  object zqrDetGudang: TZReadOnlyQuery
    Connection = DM.zConn
    SQL.Strings = (
      'SELECT a.*, b.nama, b.kode kode_gudang FROM tbl_barang_det a'
      'LEFT JOIN tbl_gudang b on a.id_gdg = b.id'
      'WHERE id_brg = :id_brg')
    Params = <
      item
        DataType = ftUnknown
        Name = 'id_brg'
