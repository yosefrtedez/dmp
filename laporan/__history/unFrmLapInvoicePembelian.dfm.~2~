      'deskripsi=deskripsi'
      'qty=qty'
      'harga=harga'
      'ppn=ppn'
      'disc_persen=disc_persen'
      'total=total'
      'no_po=no_po')
    DataSource = dsRptInvoicePembelian
    BCDToCurrency = False
    Left = 476
